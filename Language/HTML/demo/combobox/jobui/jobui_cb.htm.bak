function showAndDisplayCity(pId){
	alert(1);
	if($(pId)) {
		if($(pId).style.display == "block") {
			$(pId).style.display = "none";
		} else {
			if(pId == "indexSearchCity" || pId == "indexSearchCity2") {
				(pId == "indexSearchCity")?($(pId).innerHTML= indexSearchCity):($(pId).innerHTML= indexSearchCity2);
			}else{
				$(pId).innerHTML= indexMoreCity;
			}
			$(pId).style.display = "block";
		}
	}
}

/*首页html显示城市*/
function showAndDisplayCityHtml(pId){
	if($(pId)) {
		if($(pId).style.display == "block") {
			$(pId).style.display = "none";
		} else {
			if(pId == "indexSearchCity" ) {
				$(pId).innerHTML= indexSearchCityHtml;
			}else{
				$(pId).innerHTML= indexMoreCityHtml;
			}
			$(pId).style.display = "block";
		}
	}
}

/*首页html选中城市*/
function saveNowCityHtml(city){
	var v=city.replace(/^\s+|\s+$/ig,"");     //去掉city首尾的空白字符
	cookieSet("SEARCH_CITYKEY_COOKIE", encodeURIComponent(v), 1000*60*60*24*30, ".jobui.com");
	var re = new RegExp("(?:.+\\/)?(.+?)\\.(.+?)\\.(.+?)/","ig");
	var arr = re.exec(top.onloadUrl);
	s = RegExp.$1;
	document.location.href= "http://" + s + ".jobui.com/index_" + v + "/";
}

/*选中城市*/
function saveNowCity(city, pId, reloadWindow, url){
	var v=city.replace(/^\s+|\s+$/ig,"");     //去掉city首尾的空白字符
	if($("cityKw")) {
		$("cityKw").value = v;
	}
	if(!url) { url = location.href; reloadWindow = false; }        //若url不存在，则回到本页面
	if(url.indexOf("search.")!=-1) {
		city = encodeURIComponent(city);
		if($("jobKw")) {		
			var jobKw = $("jobKw").value;		//取得用户输入
			jobKw = encodeURIComponent(jobKw);
			url = url.replace(/jobKw=(.*?)&/ig, "jobKw="+jobKw+"&");
		}
		url = url.replace(/cityKw=(.*?)&/ig, "cityKw="+city+"&");
		reloadWindow = true;
	}
	if(pId) {
		showAndDisplayCity(pId);
	}
	if(reloadWindow) {
		cookieSet("SEARCH_CITYKEY_COOKIE", encodeURIComponent(v), 1000*60*60*24*30, ".jobui.com");
		if($("jobuiNowCity")) {
			$("jobuiNowCity").innerHTML=v;
		}
		if($("jobuiNowCity1")) {
			$("jobuiNowCity1").innerHTML=v;
		}
		if($("jobuiNowCity2")) {
			$("jobuiNowCity2").innerHTML=v;
		}
		if($("jobuiNowCity3")) {
			$("jobuiNowCity3").innerHTML=v;
		}
		if($("jobuiNowCity4")) {
			$("jobuiNowCity4").innerHTML=v;
		}
		if($("nowArea")) {//分类搜索
			$("nowArea").innerHTML=v;
		}
		if(url == location.href) {
			document.location.reload(true);
		} else {
			document.location.href=url;
		}
	} else {
		cookieSet("SEARCH_CITYKEY_COOKIE", encodeURIComponent(v), 1000*60*60*24*30, ".jobui.com");
		loadCity();
	}
}
function saveNowCityFromAjax(cityObj){
	var v=cityObj.returnMessage.replace(/^\s+|\s+$/ig,"");
	cookieSet("SEARCH_CITYKEY_COOKIE", encodeURIComponent(v), 1000*60*60*24*30, ".jobui.com");
	if($("jobuiNowCity")) {
		$("jobuiNowCity").innerHTML=v;
	}
	if($("jobuiNowCity1")) {
		$("jobuiNowCity1").innerHTML=v;
	}
	if($("jobuiNowCity2")) {
		$("jobuiNowCity2").innerHTML=v;
	}
	if($("jobuiNowCity3")) {
		$("jobuiNowCity3").innerHTML=v;
	}
	if($("jobuiNowCity4")) {
		$("jobuiNowCity4").innerHTML=v;
	}
	if($("cityKw")) {
		$("cityKw").value = v;
	}
	/*
	if($("indexHotInfo")) {
		ajaxPostDateToPage("/action/getHotForCity.php","jobuiNowCity="+r,"indexHotInfo");
	}
	*/
	
}
function loadCity(){
	var r = cookieGet("SEARCH_CITYKEY_COOKIE",true);   //从cookie取变量
	if(r && r != "") {
		if($("jobuiNowCity")) {
			$("jobuiNowCity").innerHTML=r;
		}
		if($("jobuiNowCity1")) {
			$("jobuiNowCity1").innerHTML=r;
		}
		if($("jobuiNowCity2")) {
			$("jobuiNowCity2").innerHTML=r;
		}
		if($("jobuiNowCity3")) {
			$("jobuiNowCity3").innerHTML=r;
		}
		if($("jobuiNowCity4")) {
			$("jobuiNowCity4").innerHTML=r;
		}
		if($("cityKw") && location.href.indexOf("so.jobui.com/search.")==-1) {
			$("cityKw").value = r;
		}
		if($("nowArea")) {//分类搜索
			$("nowArea").innerHTML=r;
		}
		/*
		if($("indexHotInfo")) {
			ajaxPostDateToPage("/action/getHotForCity.php","jobuiNowCity="+r,"indexHotInfo");
		}
		*/
		
	} else {
		r = ajaxGetDate("/action/getCityFromIPAjax.php", saveNowCityFromAjax, "TEXT");
	}
}
function goSearchUrl(jobkw, cityKw, industryKw) {
	document.location.href="http://so.jobui.com/search.php?jobKw="+jobkw+"&cityKw="+cityKw+"&jobSort=&indSort=0"+industryKw+"";
}
// 城市切换所需的
function hideDiv() {
	if ($("indexSearchCity")) { $("indexSearchCity").style.display = "none"; }
	if ($("indexSearchCity2")) { $("indexSearchCity2").style.display = "none"; }
	if ($("indexMoreCity")) { $("indexMoreCity").style.display = "none"; }
	if ($("choice_select")) { $("choice_select").style.display = "none"; }
	if ($("choice_select_2")) { $("choice_select_2").style.display = "none"; }
}
function cancelBubble(oEvent) {
	if (document.all) { // ie
		oEvent.cancelBubble = true;
	} else {	// ff
		oEvent.stopPropagation();
	}
}
function check(){
	if(!$("cityKw").value && ($("jobKw").getAttribute('isinputed')=='false' || !$("jobKw").value)) {
		alert("您还没有输入关键字");
		$("jobKw").focus();
		return false;
	}
	if ($("jobKw").getAttribute('isinputed')=='false') {
		$("jobKw").value="";
	}
	return true;
}

//右上角退出系统
function topRightLogout(){
	top.ajaxPostDate("/action/leftbar_exit.php","formAction=personalStatusPopupExit",
	function(){
		top.location.reload();
	});
}
//打开信息页
function showPage(){
	if(!top.jobuiLeftbarStatus) {
		top.showJobuiLeftbar();
	}
	setTimeout(function(){
		if(frames[0].openedMenuID != 6) {
			frames[0].chageMenu('6');
		}
		frames[0].chageSelection(frames[0].getEle('message_inbox').childNodes[0]);
		frames[0].changeIframeChannel('message_inbox');
	},400);
	
}


//在右上角显示"登录&注册"链接
function addTopLogin() {
	if($("divTopRight")) {
		var loginStr = '';
		if(!top.isPersonalLogin) {
			loginStr = '<a href="javascript:void(0);" onclick="javascript:if(!top.jobuiLeftbarStatus)top.showJobuiLeftbar();">登录</a>&nbsp;&nbsp;|&nbsp;&nbsp;';
		} else {
			loginStr = '<span id="topUserNickName"></span>&nbsp;&nbsp;|&nbsp;&nbsp;<a href="javascript:void(0);" onclick="topRightLogout()">退出</a>&nbsp;&nbsp;|&nbsp;&nbsp;';
			
		}
		loginStr = loginStr + $("divTopRight").innerHTML ;
		$("divTopRight").innerHTML = loginStr;
	}
}
addTopLogin();

loadCity();

//获取用户昵称
function ajaxGetTopUserNickName(){
	top.ajaxPostDate("/action/personal/personal.php","formAction=ajaxGetTopUserNickName",function(o){
		if($("topUserNickName")) {
			$("topUserNickName").innerHTML = "<font  style='font-weight: bold;'>" + o.returnMessageText + "</font>&nbsp;&nbsp;<span id='checkMessage'></span><a href='#' onclick='if(!top.jobuiLeftbarStatus)top.showJobuiLeftbar();'>我的职友集</a>";
		}	
	},"TEXT");

	//若登陆检查一次是否有新信息，有则显示
	setTimeout(function(){
		top.ajaxPostDate("/action/personal/index.php","formAction=message_checkMessage",function(o){
			var result=o.returnMessageText;
			if(result && result !="0" && result.indexOf("用户") ==-1) {
				if($("checkMessage")) {
					$("checkMessage").innerHTML = "<a href='javascript:void(0);' style='text-decoration:none' onclick='showPage();' title = '你有"+result+"条新消息,请注意查收'><img src='http://htm.jobui.com/images_so/new_msg2.gif' border='0' alt = '你有"+result+"条新消息,请注意查收'></a>&nbsp;&nbsp;";
				}
			}
	},"TEXT")},500);
}

